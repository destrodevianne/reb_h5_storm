	<template id="2" name="Silver" >
		<cost id="37000" />
 		<costWeek amount="100" />
 		<costMonth amount="300" />
		<setDouble expBonus="1.3" />
		<setDouble spBonus="1.3" />
	</template>

	<template id="3" name="Gold" >
		<cost id="37000" />
 		<costWeek amount="200" />
 		<costMonth amount="500" />
		<setDouble expBonus="1.5" />
		<setDouble spBonus="1.5" />
		<item id="57" dropMultiplier="1.3"/>
	</template>

	<template id="4" name="Platinum">
		<cost id="37000" />
 		<costWeek amount="300" />
 		<costMonth amount="700" />
		<setDouble expBonus="2" />
		<setDouble spBonus="2" />
		<setBoolean whereisVoiced="true" />
		<setBoolean playerInfinityShotArrows="true" />
		<setBoolean bufferOutsidePeace="true" />
		<setBoolean globalChat="true" />
		<item id="57" dropMultiplier="1.5"/>
	</template>

